<script type="application/ld+json">
	{
	  "@context": "https://schema.org",
	  "@type": "CollectionPage",
	  "@id": "<%= (meta.url || (VITE_SITE_URL + '/porady-eksperckie/')).replace(/\/$/, '') %>#webpage",
	  "url": "<%= meta.url || (VITE_SITE_URL + '/porady-eksperckie/') %>",
	  "name": "Porady eksperckie",
	  "description": "<%= meta.description %>",
	  "inLanguage": "pl",
	  "isPartOf": { "@id": "<%= VITE_SITE_URL %>/#website" },
	  "publisher": { "@id": "<%= VITE_SITE_URL %>/#organization" },
	  "mainEntity": {
	    "@type": "ItemList",
	    "itemListElement": [
	      <% if (meta.posts && meta.posts.length) { %>
	        <% meta.posts.forEach(function(p, i){ %>
	        {
	          "@type": "ListItem",
	          "position": <%= i + 1 %>,
	          "item": {
	            "@type": "Article",
	            "@id": "<%= p.url %>#article",
	            "url": "<%= p.url %>",
	            "headline": "<%= p.title %>",
	            "inLanguage": "pl",
	            "publisher": { "@id": "<%= VITE_SITE_URL %>/#organization" }
	          }
	        }<%= i + 1 < meta.posts.length ? ',' : '' %>
	        <% }); %>
	      <% } else { %>
	        {
	          "@type": "ListItem",
	          "position": 1,
	          "item": {
	            "@type": "Article",
	            "@id": "<%= VITE_SITE_URL %>/porady-eksperckie/zaburzenia-erekcji-a-psychika#article",
	            "url": "<%= VITE_SITE_URL %>/porady-eksperckie/zaburzenia-erekcji-a-psychika",
	            "headline": "Zaburzenia erekcji a psychika – jak emocje wpływają na sprawność seksualną?",
	            "inLanguage": "pl",
	            "publisher": { "@id": "<%= VITE_SITE_URL %>/#organization" }
	          }
	        },
	        {
	          "@type": "ListItem",
	          "position": 2,
	          "item": {
	            "@type": "Article",
	            "@id": "<%= VITE_SITE_URL %>/porady-eksperckie/dieta-wspierajaca-erekcje#article",
	            "url": "<%= VITE_SITE_URL %>/porady-eksperckie/dieta-wspierajaca-erekcje",
	            "headline": "Dieta wspierająca erekcję – co jeść, aby zadbać o zdrowie intymne?",
	            "inLanguage": "pl",
	            "publisher": { "@id": "<%= VITE_SITE_URL %>/#organization" }
	          }
	        }
	      <% } %>
	    ]
	  }
	}
</script>
