<head>
	<meta charset="UTF-8" />
	<title><%= meta.title %></title>
	<meta name="description" content="<%= meta.description %>" />
	<meta name="keywords" content="<%= meta.keywords %>" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta name="robots" content="<%= meta.robots %>" />
	<meta
		name="author"
		content="DigiDraft - Agencja Marketingowa / Interaktywna"
	/>
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<link rel="canonical" href="<%= meta.url %>" />
	<!-- OpenGraph -->
	<meta property="og:url" content="<%= meta.url %>" />
	<meta property="og:title" content="<%= meta.title %>" />
	<meta property="og:description" content="<%= meta.description %>" />
	<meta property="og:type" content="website" />
	<meta property="og:image" content="<%= meta.VITE_SITE_URL %>/og_image.webp" />
	<meta property="og:site_name" content="Vite Template" />
	<meta property="og:locale" content="pl_PL" />
	<meta property="og:image:width" content="1200" />
	<meta property="og:image:height" content="630" />
	<meta
		property="og:image:alt"
		content="Logotyp Vite Template, na jasnym tle."
	/>
	<!-- Twitter -->
	<meta name="twitter:card" content="summary_large_image" />
	<meta property="twitter:domain" content="<%= meta.VITE_SITE_URL %>" />
	<meta property="twitter:url" content="<%= meta.url %>" />
	<meta name="twitter:title" content="<%= meta.title %>" />
	<meta name="twitter:description" content="<%= meta.description %>" />
	<meta
		name="twitter:image"
		content="<%= meta.VITE_SITE_URL %>/og_image.webp"
	/>

	<meta name="format-detection" content="telephone=no" />
	<meta name="google" content="notranslate" />
	<!-- USUNĄĆ PO PRODUKCJI -->
	<meta name="robots" content="noindex, nofollow, noarchive" />
	<script
		src="https://analytics.ahrefs.com/analytics.js"
		data-key="bP5jQQzdbXOrMYsHYqp7uw"
		async
	></script>

	<!-- Organization (stałe dane firmy) -->
	<script type="application/ld+json">
		{
			"@context": "https://schema.org",
			"@type": "Organization",
			"@id": "<%= VITE_SITE_URL %>/#organization",
			"name": "IBSA Poland Sp. z o.o.",
			"legalName": "IBSA Poland Sp. z o.o.",
			"brand": { "@type": "Brand", "name": "Erectio" },
			"url": "<%= VITE_SITE_URL %>/",
			"email": "kontakt@ibsa.pl",
			"taxID": "PL5213600577",
			"identifier": {
				"@type": "PropertyValue",
				"propertyID": "KRS",
				"value": "0000383362"
			},
			"address": {
				"@type": "PostalAddress",
				"streetAddress": "ul. Jana Pawła II 29",
				"postalCode": "00-867",
				"addressLocality": "Warszawa",
				"addressCountry": "PL"
			},
			"contactPoint": [
				{
					"@type": "ContactPoint",
					"contactType": "informacje i zapytania",
					"email": "kontakt@ibsa.pl",
					"availableLanguage": ["pl"],
					"areaServed": "PL"
				}
			]
		}
	</script>

	<!-- WebSite (stały identyfikator serwisu) -->
	<script type="application/ld+json">
		{
			"@context": "https://schema.org",
			"@type": "WebSite",
			"@id": "<%= VITE_SITE_URL %>/#website",
			"url": "<%= VITE_SITE_URL %>/",
			"name": "Erectio",
			"inLanguage": "pl",
			"publisher": { "@id": "<%= VITE_SITE_URL %>/#organization" }
		}
	</script>
</head>
